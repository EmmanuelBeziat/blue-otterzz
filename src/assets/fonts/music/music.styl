@font-face {
  font-family: 'music';
  src: url('~@/assets/fonts/music/music.woff2?87680872') format('woff2'),
       url('~@/assets/fonts/music/music.woff?87680872') format('woff');
  font-weight: normal;
  font-style: normal;
}

[class^="icon-"]::before,
[class*=" icon-"]::before {
  font-family: "music";
  font-style: normal;
  font-weight: normal;
  speak: none;
  display: inline-block;
  text-decoration: inherit;
  text-align: center;
  font-variant: normal;
  text-transform: none;
  line-height: 1em;
	vertical-align: text-bottom;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
	transform: scale(.9);
}

.icon-pick::before { content: '\e800'; } /* '' */
.icon-gkey::before { content: '\e801'; } /* '' */
